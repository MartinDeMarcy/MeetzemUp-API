{
    "name": "martindemarcy/meetzemup",
    "description": "MeetZemUp Database API based on Silex 2 & Doctrine 2",
    "authors": [
        {
            "name": "martindemarcy",
            "email": "martin.pras@epitech.eu"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "silex/silex":                                              "2.0.*",
        "symfony/class-loader":                                     "2.7.*",
        "monolog/monolog":                                          "1.11.*",
        "symfony/console":                                          "2.7.*",
        "symfony/validator":                                        "2.7.*",
        "doctrine/dbal":                                            "2.5.*",
        "symfony/doctrine-bridge":                                  "2.7.*",
        "dflydev/doctrine-orm-service-provider":                    "2.0.*",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider":   "3.0.0",
        "symfony/yaml":                                             "2.7.*",
        "symfony/form":                                             "2.7.*",
        "symfony/config":                                           "2.7.*",
        "symfony/translation":                                      "2.7.*",
        "symfony/intl":                                             "2.7.*",
        "symfony/security":                                         "2.7.*",
        "phpunit/phpunit":                                          "4.4.*",
        "symfony/browser-kit":                                      "2.7.*",
        "useallfive/doctrine-data-fixtures-command":                "dev-master",
        "swiftmailer/swiftmailer":                                  "5.2.2",
        "league/oauth2-server":                                     "6.*"
    },
    "autoload":    {
        "psr-0": {
            "Controller": "src/",
            "Model": "src/",
            "Repository": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "chmod 0755 logs/",
            "chmod 0755 cache/"
        ]
    }
}
